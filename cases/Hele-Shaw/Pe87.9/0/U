FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    up1
    {
        type            noSlip;
    }
    down
    {
        type            noSlip;
    }
    outlet
    {
        type            zeroGradient;
    }
    up2
    {
        type            noSlip;
    }

	inlet
    {
        type            flowRateInletVelocity;
        volumetricFlowRate constant 1.66667E-08;
        extrapolateProfile false;
        value           uniform (0 0 0);
    }
}


