FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Y;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions     [1 -3 0 0 0 0 0];

internalField   uniform 317.6;

boundaryField
{
    up1
    {
        type            zeroGradient;
    }
    down
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    up2
    {
        type            zeroGradient;
    }
	inlet
    {
        type            fixedValue;
        value           uniform 285.6;
    }
}


