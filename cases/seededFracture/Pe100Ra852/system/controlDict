FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dt 500;
etime 15000;

application     dissolGravityFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         $etime;

deltaT          0.01;

writeControl    adjustableRunTime;

writeInterval   $dt;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression on;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo          10;

maxDeltaT       1;



